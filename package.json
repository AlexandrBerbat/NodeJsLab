{
  "name": "nodejslab",
  "version": "1.0.0",
  "description": "Lab Web Node JS",
  "main": "index.js",
  "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
  "precommit": "npm run prettier",
  "prepush": "call npm run lint && call npm run test",
  "scripts": {
    "start": "node index.js",
    "prettier": "prettier --config .prettierrc.json --write src/**/*.js",
    "doc": "esdoc --out docs/src/ --readme ./README.md",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": ".eslintrc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexandrBerbat/NodeJsLab.git"
  },
  "husky": {
    "hooks": {
      "precommit": "npm run precommit",
      "prepush": "echo 'pushing'"
    }
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexandrBerbat/NodeJsLab/issues"
  },
  "homepage": "https://github.com/AlexandrBerbat/NodeJsLab#readme",
  "dependencies": {
    "lodash": "^4.17.15",
    "make": "^0.8.1"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.28",
    "esdoc": "^1.1.0",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1"
  }
}
